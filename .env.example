# Sei Network Configuration
VITE_SEI_MAINNET_RPC=https://evm-rpc.sei-apis.com
VITE_SEI_TESTNET_RPC=https://evm-rpc-testnet.sei-apis.com
VITE_USE_TESTNET_FOR_SEILIST=false

# Reown Wallet Integration
VITE_REOWN_PROJECT_ID=your_project_id_here
VITE_REOWN_APP_ID=your_app_id_here
VITE_REOWN_RELAY_URL=wss://relay.reown.com

# Development Configuration (Optional)
VITE_DEV_WALLET=your_dev_wallet_address
VITE_DEV_WALLET_PRIVATE_KEY=your_dev_private_key_for_testing

# Token Factory Configuration
VITE_FACTORY_ADDRESS_TESTNET=your_testnet_factory_address
VITE_FACTORY_ADDRESS_MAINNET=your_mainnet_factory_address

# API Keys (Optional for enhanced features)
VITE_COINGECKO_API_KEY=your_coingecko_api_key
VITE_DEXSCREENER_API_KEY=your_dexscreener_api_key

# Security Features
VITE_ENABLE_ADVANCED_SCANNING=true
VITE_ENABLE_REAL_TIME_ALERTS=true
VITE_ENABLE_BAD_ACTOR_DETECTION=true